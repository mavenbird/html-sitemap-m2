        <referenceBlock name="header.links">
            <block class="Mavenbird\HtmlSiteMap\Block\ItemsCollection" name="custom_block_top" as="custom_block_top" template="Mavenbird_HtmlSiteMap::custom_top.phtml" before="-"/>
